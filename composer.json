{
  "name": "enabel/password",
  "description": "A php application for Active Directory Manipulation",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Damien Lagae",
      "email": "damien.lagae@enabel.be"
    }
  ],
  "type": "project",
  "autoload": {
    "psr-4": {
      "AuthBundle\\": "src/AuthBundle",
      "BisBundle\\": "src/BisBundle",
      "App\\": "src/",
      "Api\\": "src/ApiBundle"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "AuthBundle\\Tests\\": "tests/AuthBundle"
    }
  },
  "require": {
    "php": "^7.2",
    "adldap2/adldap2": "10.1.0",
    "btc/flagicon-bundle": "^1.3",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "doctrine/orm": "^2.7",
    "fzaninotto/faker": "^1.7",
    "guzzlehttp/psr7": "^1.4",
    "jms/i18n-routing-bundle": "^3.0",
    "misd/phone-number-bundle": "^1.3",
    "php-http/guzzle6-adapter": "^1.1",
    "php-http/httplug-bundle": "^1.14",
    "php-translation/loco-adapter": "^0.11.0",
    "php-translation/symfony-bundle": "^0.12.1",
    "predis/predis": "^1.1",
    "sensio/framework-extra-bundle": "^5.0",
    "smsgatewayme/client": "^0.0.1",
    "snc/redis-bundle": "^3.2",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "4.4.*",
    "symfony/cache": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/flex": "^1.1",
    "symfony/form": "4.4.*",
    "symfony/framework-bundle": "4.4.*",
    "symfony/http-client": "4.4.*",
    "symfony/messenger": "4.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/polyfill-apcu": "^1.0",
    "symfony/requirements-checker": "^1.1",
    "symfony/security-bundle": "4.4.*",
    "symfony/security-csrf": "4.4.*",
    "symfony/security-http": "4.4.*",
    "symfony/serializer-pack": "^1.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "4.4.*",
    "symfony/translation": "4.4.*",
    "symfony/twig-bundle": "4.4.*",
    "symfony/validator": "4.4.*",
    "symfony/yaml": "4.4.*",
    "twig/extra-bundle": "^3.0",
    "twig/twig": "~2.0",
    "twilio/sdk": "^6.1"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^3.5",
    "friendsofphp/php-cs-fixer": "^2.15",
    "friendsoftwig/twigcs": "^3.1",
    "php-parallel-lint/php-console-highlighter": "^0.4",
    "php-parallel-lint/php-parallel-lint": "^1.0",
    "phpmd/phpmd": "^2.6",
    "phpro/grumphp": "^0.15",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan": "^0.12.7",
    "phpstan/phpstan-doctrine": "^0.12.9",
    "phpstan/phpstan-mockery": "^0.12.3",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-symfony": "^0.12.7",
    "sebastian/phpcpd": "^4.1",
    "sensiolabs/security-checker": "^5.0",
    "squizlabs/php_codesniffer": "^3.0",
    "symfony/browser-kit": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/debug-pack": "^1.0",
    "symfony/maker-bundle": "^1.14",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "^1.0",
    "vimeo/psalm": "^3.10"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "config": {
    "process-timeout": 0,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "4.4.*"
    },
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
