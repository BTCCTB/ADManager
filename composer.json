{
    "name": "enabel/password",
    "description": "A php application for Active Directory Manipulation",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Damien Lagae",
            "email": "damienlagae@gmail.com"
        }
    ],
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "AuthBundle\\": "src/AuthBundle",
            "BisBundle\\": "src/BisBundle",
            "App\\": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\AppBundle\\": "tests/AppBundle",
            "Test\\AuthBundle\\": "tests/AuthBundle",
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "adldap2/adldap2": "^8.1",
        "btc/flagicon-bundle": "^1.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5",
        "php-http/guzzle6-adapter": "^1.1",
        "php-translation/loco-adapter": "^0.5.0",
        "php-translation/symfony-bundle": "^0.6.1",
        "sensio/framework-extra-bundle": "^4.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/security-csrf": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/templating": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/yaml": "^4.0",
        "twig/twig": "^1.0||^2.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "nikic/php-parser": "^3.1",
        "phpmd/phpmd": "^2.6",
        "phpro/grumphp": "^0.13",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "process-timeout": 0,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
