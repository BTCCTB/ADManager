{
  "name": "enabel/password",
  "description": "A php application for Active Directory Manipulation",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Damien Lagae",
      "email": "damien.lagae@enabel.be"
    }
  ],
  "type": "project",
  "autoload": {
    "psr-4": {
      "Api\\": "src/Api",
      "App\\": "src/",
      "Auth\\": "src/Auth",
      "Bis\\": "src/Bis"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/",
      "Auth\\Tests\\": "tests/Auth"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
